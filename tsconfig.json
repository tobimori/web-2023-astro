{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "jsx": "react-jsx",
    "jsxImportSource": "react",
    "baseUrl": ".",
    "paths": {
      "@components": ["src/components"],
      "@components/*": ["src/components/*"],
      "@styles": ["src/styles"],
      "@styles/*": ["src/styles/*"],
      "@lib": ["src/lib"],
      "@lib/*": ["src/lib/*"],
      "@layouts": ["src/layouts"],
      "@layouts/*": ["src/layouts/*"]
    }
  }
}
